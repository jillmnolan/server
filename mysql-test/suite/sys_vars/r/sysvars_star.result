set auto_increment_increment=10;
set global auto_increment_offset=5;
install soname 'query_response_time';
select *  from information_schema.system_variables
where variable_name in (
'auto_increment_increment',       #session!=global, origin=compile-time
'auto_increment_offset',          #global!=default, origin=sql
'column_compression_threshold',   #origin=command-line
'secure_file_priv',               #origin=config
'query_response_time_range_base', #plugin, origin=command-line
'query_response_time_stats'       #plugin, origin=config
)
order by variable_name;
VARIABLE_NAME	AUTO_INCREMENT_INCREMENT
SESSION_VALUE	10
GLOBAL_VALUE	1
GLOBAL_VALUE_ORIGIN	COMPILE-TIME
DEFAULT_VALUE	1
VARIABLE_SCOPE	SESSION
VARIABLE_TYPE	BIGINT UNSIGNED
VARIABLE_COMMENT	Auto-increment columns are incremented by this
NUMERIC_MIN_VALUE	1
NUMERIC_MAX_VALUE	65535
NUMERIC_BLOCK_SIZE	1
ENUM_VALUE_LIST	NULL
READ_ONLY	NO
COMMAND_LINE_ARGUMENT	OPTIONAL
GLOBAL_VALUE_PATH	NULL
VARIABLE_NAME	AUTO_INCREMENT_OFFSET
SESSION_VALUE	1
GLOBAL_VALUE	5
GLOBAL_VALUE_ORIGIN	SQL
DEFAULT_VALUE	1
VARIABLE_SCOPE	SESSION
VARIABLE_TYPE	BIGINT UNSIGNED
VARIABLE_COMMENT	Offset added to Auto-increment columns. Used when auto-increment-increment != 1
NUMERIC_MIN_VALUE	1
NUMERIC_MAX_VALUE	65535
NUMERIC_BLOCK_SIZE	1
ENUM_VALUE_LIST	NULL
READ_ONLY	NO
COMMAND_LINE_ARGUMENT	OPTIONAL
GLOBAL_VALUE_PATH	NULL
VARIABLE_NAME	COLUMN_COMPRESSION_THRESHOLD
SESSION_VALUE	120
GLOBAL_VALUE	120
GLOBAL_VALUE_ORIGIN	COMMAND-LINE
DEFAULT_VALUE	100
VARIABLE_SCOPE	SESSION
VARIABLE_TYPE	INT UNSIGNED
VARIABLE_COMMENT	Minimum column data length eligible for compression
NUMERIC_MIN_VALUE	0
NUMERIC_MAX_VALUE	4294967295
NUMERIC_BLOCK_SIZE	1
ENUM_VALUE_LIST	NULL
READ_ONLY	NO
COMMAND_LINE_ARGUMENT	REQUIRED
GLOBAL_VALUE_PATH	NULL
VARIABLE_NAME	QUERY_RESPONSE_TIME_RANGE_BASE
SESSION_VALUE	NULL
GLOBAL_VALUE	16
GLOBAL_VALUE_ORIGIN	COMMAND-LINE
DEFAULT_VALUE	10
VARIABLE_SCOPE	GLOBAL
VARIABLE_TYPE	BIGINT UNSIGNED
VARIABLE_COMMENT	Select base of log for query_response_time ranges. WARNING: variable change affect only after flush
NUMERIC_MIN_VALUE	2
NUMERIC_MAX_VALUE	1000
NUMERIC_BLOCK_SIZE	1
ENUM_VALUE_LIST	NULL
READ_ONLY	NO
COMMAND_LINE_ARGUMENT	REQUIRED
GLOBAL_VALUE_PATH	NULL
VARIABLE_NAME	QUERY_RESPONSE_TIME_STATS
SESSION_VALUE	NULL
GLOBAL_VALUE	ON
GLOBAL_VALUE_ORIGIN	CONFIG
DEFAULT_VALUE	OFF
VARIABLE_SCOPE	GLOBAL
VARIABLE_TYPE	BOOLEAN
VARIABLE_COMMENT	Enable or disable query response time statisics collecting
NUMERIC_MIN_VALUE	NULL
NUMERIC_MAX_VALUE	NULL
NUMERIC_BLOCK_SIZE	NULL
ENUM_VALUE_LIST	OFF,ON
READ_ONLY	NO
COMMAND_LINE_ARGUMENT	OPTIONAL
GLOBAL_VALUE_PATH	mysql-test/var/my.cnf
VARIABLE_NAME	SECURE_FILE_PRIV
SESSION_VALUE	NULL
GLOBAL_VALUE	mysql-test/var/
GLOBAL_VALUE_ORIGIN	CONFIG
DEFAULT_VALUE	
VARIABLE_SCOPE	GLOBAL
VARIABLE_TYPE	VARCHAR
VARIABLE_COMMENT	Limit LOAD DATA, SELECT ... OUTFILE, and LOAD_FILE() to files within specified directory
NUMERIC_MIN_VALUE	NULL
NUMERIC_MAX_VALUE	NULL
NUMERIC_BLOCK_SIZE	NULL
ENUM_VALUE_LIST	NULL
READ_ONLY	YES
COMMAND_LINE_ARGUMENT	REQUIRED
GLOBAL_VALUE_PATH	mysql-test/var/my.cnf
create user foo@localhost;
connect foo,localhost,foo;
select * from information_schema.system_variables
where global_value_path is not null;
connection default;
drop user foo@localhost;
set global auto_increment_offset=default;
uninstall soname 'query_response_time';
